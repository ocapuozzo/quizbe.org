{# template for response prototype (sub-form) : see Question:edit.html.twig #}
<div>      
  <div class="form-group">
    {{ form_label(form.proposition, 'proposition'|trans ~ ':', {'label_attr': {'class': 'col-sm-2 control-label'}})}}
    {{ form_errors(form.proposition) }}
    <div class="col-sm-6">
      {{ form_widget(form.proposition, {'attr': {'class': 'form-control'}}) }}
    </div>
    {{ form_label(form.value, 'value'|trans ~ ':', {'label_attr': {'class': 'col-sm-2 control-label'}})}}
    {{ form_errors(form.value) }}
    <div class="col-sm-2">
      {{ form_widget(form.value, {'attr': {'class': 'form-control'}}) }}
    </div>    
  </div>
  <div class="form-group">
    {{ form_label(form.feedback, 'feedback'|trans ~ ':', {'label_attr': {'class': 'col-sm-2 control-label'}})}}
    {{ form_errors(form.feedback) }}
    <div class="col-sm-10">
      {{ form_widget(form.feedback, {'attr': {'class': 'form-control'}}) }}
    </div>
  </div>  
</div>
    